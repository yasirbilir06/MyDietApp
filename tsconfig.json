{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-native",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "noEmit": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "types": ["react", "react-native", "expo"]
    
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
